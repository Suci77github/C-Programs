#include <stdio.h>

void swap(int, int);
intmain()
{
int x, y;

printf("Enter the value of x and y\n");
scanf("%d%d",&x,&y);

printf("Before Swapping x = %d y = %d\n", x, y);
swap(x, y); 
printf("After Swapping x = %d y = %d\n", x, y);
   return 0;
}

void swap(int a, int b)
{
int temp;
   temp = b;
   b = a;
   a = temp;
printf("Values of a and b is %d  %d\n",a,b);
}
